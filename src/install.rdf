<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>{bbee9373-4135-47cb-b98e-98e62085837f}</em:id>
    <em:version>4.7.0</em:version>
    <em:name>Fox 2 The Moon</em:name>
    <em:description>The default Firefox 2 theme for UXP.</em:description>
    <em:creator>Lootyhoof</em:creator>
    <em:type>4</em:type>
    <em:internalName>f2tm</em:internalName>
    <em:skinnable>true</em:skinnable>
    <em:updateURL>https://raw.githubusercontent.com/Lootyhoof/fox2themoon/master/update.xml</em:updateURL>

    <em:contributor>fax8</em:contributor>
    <em:contributor>rctgamer3</em:contributor>
    <em:contributor>borr</em:contributor>
    <em:contributor>Graciliano (Win10 caption buttons)</em:contributor>

    <em:targetApplication>
      <!-- Pale Moon -->
      <Description>
        <em:id>{8de7fcbb-c55c-4fbe-bfc5-fc555c87dbc4}</em:id>
        <em:minVersion>28.6.0</em:minVersion>
        <em:maxVersion>30.*</em:maxVersion>
      </Description>
    </em:targetApplication>
    <em:targetApplication>
      <!-- Applications using Firefox GUID -->
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <!--
            Pale Moon 30 and above will stop using its own GUID to
            "further improve extension compatibility"
          -->
        <em:minVersion>30.0.0a1</em:minVersion>
        <em:maxVersion>30.*</em:maxVersion>
      </Description>
    </em:targetApplication>
    
    <em:targetApplication>
      <Description>
        <!-- Ambassador -->
        <em:id>{4523665a-317f-4a66-9376-3763d1ad1978}</em:id>
        <em:minVersion>1.0</em:minVersion>
        <em:maxVersion>1.*</em:maxVersion>
      </Description>
    </em:targetApplication>
  </Description>
</RDF>
